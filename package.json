  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "morgan": "^1.9.0",
    "qs": "^6.5.1",
    "redis": "^2.8.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
